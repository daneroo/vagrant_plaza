<VirtualHost *:80>
DocumentRoot <%= @node[:vagrant][:config][:vm][:shared_folders]["ekoform-dev"][:guestpath] %>/web
ServerName ekoform.dev.axialdev.net

SetEnv axAppConfigFile '<%= @node[:vagrant][:directory] %>/appConfig-ekoform-dev.json'

ErrorLog <%= node[:apache][:log_dir] %>/ekoform-dev-error.log
LogLevel warn
LogFormat "%v %l %u %t \"%r\" %>s %b" comonvhost
CustomLog <%= node[:apache][:log_dir] %>/ekoform-dev-access.log comonvhost
</VirtualHost>
	